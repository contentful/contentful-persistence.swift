FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/Contentful-iOS" "$PODS_CONFIGURATION_BUILD_DIR/Interstellar-iOS" "$PODS_CONFIGURATION_BUILD_DIR/ObjectMapper-iOS"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks' '@executable_path/../../Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/Contentful-iOS/Contentful.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Interstellar-iOS/Interstellar.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/ObjectMapper-iOS/ObjectMapper.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "Contentful" -framework "Interstellar" -framework "ObjectMapper"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
